	/*****************************config_v821************************************/
	"config_v821": {
		"desc": "Set Driver config for V821",
		"force_config": true,
		"depends": [
			"config_v821_sip_wifi"
		],
		"kernel": [
			"CONFIG_CHIP_V821=y",
			"# CONFIG_CHIP_V821B is not set"
		],
		"rtos": [
			"CONFIG_CHIP_V821=y",
			"# CONFIG_CHIP_V821B is not set"
		]
	},
