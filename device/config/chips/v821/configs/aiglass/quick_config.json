	/*****************************one_gc1084_sensor************************************/
	"one_gc1084_sensor": {
		"desc": "set one camare of gc1084 online for this board",
		"board.dts": {
			"set_property": {
				//s0vencfg0
				"s0vencfg0": {
					"status": "\"okay\"",
					"res_w": "<1280>",
					"res_h": "<720>",
					"bit_rate": "<1024>",
					"jpg_w": "<1280>",
					"jpg_h": "<720>"
				},
				//s1vencfg0
				"s1vencfg0": {
					"status": "\"disabled\""
				},
				//s2vencfg0
				"s2vencfg0": {
					"status": "\"disabled\""
				},
				//csi
				"csi1": {
					"pinctrl-0": "<&ncsi_bt656_pins_a>",
					"pinctrl-1": "<&ncsi_bt656_pins_b>",
					"status": "\"disabled\""
				},
				//tdm
				"tdm0": {
					"work_mode": "<0x0>"
				},
				//isp
				"isp00": {
					"work_mode": "<0x0>"
				},
				"isp01": {
					"status": "\"disabled\""
				},
				"isp10": {
					"status": "\"disabled\""
				},
				//scaler
				"scaler00": {
					"work_mode": "<0x0>"
				},
				"scaler01": {
					"status": "\"disabled\""
				},
				"scaler10": {
					"work_mode": "<0x0>",
					"status": "\"okay\""
				},
				"scaler11": {
					"status": "\"disabled\""
				},
				//set sensor name and addr
				"sensor0": {
					"sensor0_mname": "\"gc1084_mipi\"",
					"sensor0_twi_addr": "<0x6e>",
					"status": "\"okay\""
				},
				"sensor1": {
					"status": "\"disabled\""
				},
				//set /dev/video0/4
				"vinc00": {
					"work_mode": "<0x0>",
					"status": "\"okay\""
				},
				"vinc01": {
					"vinc1_rear_sensor_sel": "<0>",
					"vinc1_front_sensor_sel": "<0>",
					"status": "\"disabled\""
				},
				"vinc10": {
					"work_mode": "<0x0>",
					"status": "\"okay\""
				},
				"vinc11": {
					"vinc5_rear_sensor_sel": "<0>",
					"vinc5_front_sensor_sel": "<0>",
					"status": "\"disabled\""
				}
			}
		},
		// enable sensor complile
		"kernel": [
			"# CONFIG_SUPPORT_ISP_TDM is not set",
			"CONFIG_SENSOR_GC1084_MIPI=y",
			"# CONFIG_SENSOR_GC2083_MIPI is not set"
		],
		// rtos configs
		"rtos_sysconfig": {
			"vind/sensor0": {
				"sensor0_mname": "\"gc1084_mipi\""
			},
			"vind/vinc0": {
				"vinc0_mipi_num": "1"
			},
			"vind/vinc4": {
				"vinc4_mipi_num": "1"
			}
		},
		// rtos configs
		"rtos": [
			"# CONFIG_SENSOR_GC2083_MIPI is not set",
			"CONFIG_SENSOR_GC1084_MIPI=y"
		]
	}, //end of one_gc1084_sensor
