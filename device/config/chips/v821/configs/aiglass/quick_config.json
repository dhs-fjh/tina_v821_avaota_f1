	/*****************************one_gc1084_sensor(2in1)************************************/
	"one_gc1084_sensor_(2in1)": {
		"desc": "Set one camare of gc1084 online for this board (2in1 mode)",
		"board.dts": {
			"set_property": {
				"rv_ddr_reserved": {
					"reg": "<0x0 0x81000000 0x0 0x440000>"
				},
				"rv_vdev0buffer": {
					"reg": "<0x0 0x81440000 0x0 0x40000>"
				},
				"rv_vdev0vring0": {
					"reg": "<0x0 0x81480000 0x0 0x2000>"
				},
				"e907_mem_fw": {
					"reg": "<0x0 0x81484000 0x0 0x0025B000>"
				},
				"e907_share_irq_table": {
					"reg": "<0x0 0x816EE000 0x0 0x2000>"
				},
				"e907_rpbuf_reserved": {
					"reg": "<0x0 0x816F0000 0x0 0x40000>"
				},
				"isp_dram_reserved": {
					"reg": "<0x0 0x81730000 0x0 0x008d0000>"
				},
				"e907_rproc": {
					"fw-partition-sectors": "<4864>"
				},
				"tdm0": {
					"work_mode": "<0x1>"
				},
				"isp00": {
					"work_mode": "<0x1>"
				},
				"isp01": {
					"status": "\"okay\"",
					"rpbuf": "<&rpbuf_controller0>",
					"rpmsg-ser-name": "\"43030000.e907_rproc\""
				},
				"scaler00": {
					"work_mode": "<0x1>"
				},
				"scaler01": {
					"rpmsg-ser-name": "\"43030000.e907_rproc\"",
					"status": "\"okay\""
				},
				"scaler10": {
					"work_mode": "<0x1>"
				},
				"scaler11": {
					"rpmsg-ser-name": "\"43030000.e907_rproc\"",
					"status": "\"okay\""
				},
				"vinc00": {
					"work_mode": "<0x1>"
				},
				"vinc01": {
					"vinc1_csi_sel": "<0>",
					"vinc1_mipi_sel": "<0>",
					"rpmsg-ser-name": "\"43030000.e907_rproc\"",
					"delay_init": "<1>",
					"status": "\"okay\""
				},
				"vinc10": {
					"work_mode": "<0x1>"
				},
				"vinc11": {
					"vinc5_csi_sel": "<0>",
					"vinc5_mipi_sel": "<0>",
					"rpmsg-ser-name": "\"43030000.e907_rproc\"",
					"delay_init": "<1>",
					"status": "\"okay\""
				}
			}
		},
		"kernel": [
			"CONFIG_SUPPORT_ISP_TDM=y",
			"CONFIG_VIN_SENSOR_RESERVE_ADDR=0x81FFE000"
		],
		"partition_nor": [
			{
				"name": "riscv0",
				"size": "4480"
			}
		],
		// rtos configs
		"rtos_sysconfig": {
			"vind/vinc0": {
				"vinc0_tdm_rx_sel": "0",
				"vinc0_width": "320",
				"vinc0_height": "240",
				"vinc0_merge_width": "1280",
				"vinc0_merge_height": "720",
				"vinc0_work_mode": "1"
			},
			"vind/vinc1": {
				"vinc1_used": "1",
				"vinc1_isp_sel": "1",
				"vinc1_tdm_rx_sel": "1",
				"vinc1_width": "320",
				"vinc1_height": "240",
				"vinc1_merge_width": "1280",
				"vinc1_merge_height": "720",
				"vinc1_use_sensor_list": "0",
				"vinc1_mipi_num": "1"
			}
		},
		// rtos configs
		"rtos": [
			"CONFIG_ARCH_MEM_LENGTH=0x440000",
			"CONFIG_ISP_NUMBER=2",
			"CONFIG_TDM_RX_BUF_NUM=1",
			"CONFIG_VIN_WAIT_AMP_INIT_SEM_TIMEOUT=4000",
			"CONFIG_ISP_MEMRESERVE_ADDR=0x81730000",
			"CONFIG_ISP_MEMRESERVE_LEN=0x8d0000",
			"CONFIG_RTOS_FW_RESERVED_MEM_ADDR=0x81484000"
		]
	}, //end of one_gc1084_sensor
