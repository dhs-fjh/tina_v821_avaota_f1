			"CONFIG_AG_LINK=y",
			"CONFIG_AG_UART=y"
		],
		"openwrt": [
			"CONFIG_rt_media-mpp_demo_ai_glass=y",
			"CONFIG_demo_ai_glass_autostart=y",
			"# CONFIG_rt_media_demo_video_in is not set"
		]
	}, //end of ai_glass_app
	/*****************************pl2_pl3_uart3*************************/
	"pl2_pl3_uart3": {
		"desc": "Set debug uart to PL2 PL3 with UART3",
		"board.dts": {
			"bootargs": {
				"earlyprintk": "sunxi-uart,0x42500c00",
				"console": "ttyS3,1500000"
			},
			"set_property": {
				"uart3": {
					"pinctrl-0": "<&r_uart3_pins_default>",
					"pinctrl-1": "<&r_uart3_pins_sleep>",
					"status": "\"okay\""
				},
				"uart0": {
					"status": "\"disabled\""
				}
			}
		},
		"sysconfig": {
			"uart_para": {
				"uart_debug_port": 3,
				"uart_debug_tx": "port:PL02<3><1><default><default>",
				"uart_debug_rx": "port:PL03<3><1><default><default>"
			}
		},
		"BoardConfig": {
			"LICHEE_BRANDY_DEFCONF": "sun300iw1p1_v821_uart3_defconfig"
		},
		"BoardConfig_nor": {
			"LICHEE_BRANDY_DEFCONF": "sun300iw1p1_v821_uart3_defconfig"
		}
	}, // pl2_pl3_uart3

	"switch_to_nor" : {
		"desc" : "Switch storage type to spinor",
		"finish_note": [
			"change flash to emmc successful.\n",
			"Please run those command\n",
			"source build/envsetup.sh",
			"lunch",
			"mrtos menuconfig",
			"mrtos clean\n",
			"to apply this setting"
		],
		"configs": { "LICHEE_FLASH": "nor" },
		"board.dts": {
			"bootargs": {
				"root": "/dev/mtdblock5",
				"boot_type": "3",
				"mbr_offset": "311296"
			},
			"set_property": {
				"isp_param": { "partition_name" : "\"/dev/mtd5\"" },
				"sdc0" : { "status" : "\"okay\"" },
				"spif0" : { "status" : "\"okay\"" }
			}
		},
		"sysconfig": { "target": { "storage_type": 3 } },
		"uboot-board.dts" : { "set_property": { "target": { "storage_type": "<3>" } } },
		"kernel": [
			"CONFIG_AW_MTD=y",
			"CONFIG_AW_MTD_SUNXI_PARTS=y",
			"CONFIG_AW_MTD_SPI_NOR_5_4=y",
			"CONFIG_SPINOR_UBOOT_OFFSET=280",
			"# CONFIG_MTD_SPI_NOR_USE_4K_SECTORS is not set",
			"CONFIG_SPIF_SUNXI=y",
			"CONFIG_AW_SPI_NG=y",
			"# CONFIG_AW_SPI_NG_DBI is not set",
			"# CONFIG_AW_SPI_NG_CAMERA is not set",
			"CONFIG_AW_SPI_NG_ATOMIC_XFER=y",
			"CONFIG_SPI_SPIDEV=y",
			"CONFIG_SPI_SLAVE=y",
			"CONFIG_JFFS2_FS=y"
		],
		"openwrt": [ "optimize_rootfs_nor.fragment" ],
		"partition_nor" : [ { "name" : "mbr" } ], // use auto to update partition cmdline
		"cmd" : [
			"mkdir -p ${LICHEE_PLAT_OUT}/ota_sdnand",
			"cp ${LICHEE_PACK_OUT_DIR}/boot0_sdcard.fex ${LICHEE_PLAT_OUT}/ota_sdnand",
			"cp ${LICHEE_PACK_OUT_DIR}/boot_package.fex ${LICHEE_PLAT_OUT}/ota_sdnand",
			"cp ${LICHEE_PACK_OUT_DIR}/boot.fex ${LICHEE_PLAT_OUT}/ota_sdnand",
			"cp ${LICHEE_PACK_OUT_DIR}/rootfs.fex ${LICHEE_PLAT_OUT}/ota_sdnand",
			"cp ${LICHEE_PACK_OUT_DIR}/amp_rv0.fex ${LICHEE_PLAT_OUT}/ota_sdnand",
			"cp ${LICHEE_PACK_OUT_DIR}/boot.fex ${LICHEE_PLAT_OUT}/boot.default",
			"cp ${LICHEE_PACK_OUT_DIR}/rootfs.fex ${LICHEE_PLAT_OUT}/rootfs.default",
			"cp ${LICHEE_PLAT_OUT}/.buildconfig ${LICHEE_PLAT_OUT}/.buildconfig_default"
		]
	},

