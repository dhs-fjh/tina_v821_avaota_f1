					"heap-size": "<0x01700000>",
					"thrs": "<100>",
					"sizes": "<0 23552>"
				},
				//csi
				"csi1": {
					"pinctrl-0": "<>",
					"pinctrl-1": "<>",
					"status": "\"okay\""
				},
				//tdm
				"tdm0": {
					"work_mode": "<0x1>"
				},
				//isp
				"isp00": {
					"work_mode": "<0x1>"
				},
				"isp01": {
					"status": "\"okay\"",
					"ldci_select": "<0x1>"
				},
				"isp10": {
					"status": "\"okay\""
				},
				//scaler
				"scaler00": {
					"work_mode": "<0x1>"
				},
				"scaler01": {
					"status": "\"okay\""
				},
				"scaler10": {
					"work_mode": "<0x1>",
					"status": "\"okay\""
				},
				"scaler11": {
					"status": "\"okay\""
				},
				//set sensor name and addr
				"sensor0": {
					"sensor0_mname": "\"gc1084_mipi\"",
					"sensor0_twi_addr": "<0x6e>",
					"status": "\"okay\""
				},
				"sensor1": {
					"sensor1_mname": "\"gc1084_mipi_2\"",
					"sensor1_twi_addr": "<0x7e>",
					"status": "\"okay\""
				},
				// set /dev/video0/1/4/5
				"vinc00": {
					"work_mode": "<0x1>",
					"status": "\"okay\""
				},
				"vinc01": {
					"vinc1_rear_sensor_sel": "<1>",
					"vinc1_front_sensor_sel": "<1>",
					"status": "\"okay\""
				},
				"vinc10": {
					"work_mode": "<0x1>",
					"status": "\"okay\""
				},
				"vinc11": {
					"vinc5_rear_sensor_sel": "<1>",
					"vinc5_front_sensor_sel": "<1>",
					"status": "\"okay\""
				}
			}
		},
